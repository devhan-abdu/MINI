{
  "expo": {
    "name": "Mini",
    "slug": "mini",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/minilogo.png",
    "scheme": "mini",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
    "image": "./assets/images/minilogo.png",
    "resizeMode": "contain",
    "backgroundColor": "#276359" 
   },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devhananabdu.mini"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.devhananabdu.mini"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/minilogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#276359",
          "dark": {
            "backgroundColor": "#1e293b"
          }
        }
      ],
      "expo-sqlite",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "886f0dc1-bf5a-4a08-a916-c5aa4bd80362"
      }
    },
    "owner": "devhan-abdu",
    "updates": {},
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
